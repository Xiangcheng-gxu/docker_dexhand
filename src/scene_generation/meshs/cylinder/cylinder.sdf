<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="cylinder">
    <static>false</static>
    <link name="link">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
        <inertia>
          <ixx>0.00004167</ixx>  <!-- 圆柱体惯性矩已根据其几何形状重新计算 -->
