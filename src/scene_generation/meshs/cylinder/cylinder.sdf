          <cylinder>
            <radius>0.02</radius>   <!-- 圆柱体半径 -->
            <length>0.04</length>     <!-- 圆柱体高度 -->
          </cylinder>
        </geometry>
        <pose>0 0 0 0 0 0</pose>     <!-- 确保碰撞体与视觉体位置一致 -->
        <surface>
          <contact>
            <ode/>
          </contact>
          <friction>
            <ode>
              <mu>1.0</mu>
              <mu2>1.0</mu2>
            </ode>
          </friction>
