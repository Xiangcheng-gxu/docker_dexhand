          <cylinder>
            <radius>0.02</radius>    <!-- 与碰撞体尺寸一致 -->
            <length>0.1</length>
          </cylinder>
        </geometry>
        <pose>0 0 0 0 0 0</pose>      <!-- 与碰撞体姿态一致 -->
        <material>
          <ambient>0.1 0.8 0.1 1</ambient>  <!-- 绿色环境光 -->
          <diffuse>0.1 0.8 0.1 1</diffuse>   <!-- 绿色漫反射 -->
